<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <!--com.squareup.timessquare.CalendarPickerView
            android:id="@+id/calendar_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintBottom_toTopOf="@+id/guideline52" app:layout_constraintHorizontal_bias="0.0"
    /-->

    <Button
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/create"
            android:background="#FFB74D"
            app:layout_constraintTop_toTopOf="@+id/guideline53" app:layout_constraintStart_toStartOf="@+id/guideline32"
            app:layout_constraintEnd_toStartOf="@+id/guideline54"
            app:layout_constraintBottom_toTopOf="@+id/guideline55" android:text="OK"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline32"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.35"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline35"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.5"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline52"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.80"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline53"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.85"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline54"
                                          android:orientation="vertical" app:layout_constraintGuide_percent="0.65"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline55"
                                          android:orientation="horizontal" app:layout_constraintGuide_percent="0.95"/>


